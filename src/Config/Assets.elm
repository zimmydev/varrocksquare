module Config.Assets exposing (icon, image, radio)

import Dict exposing (Dict)
import Element exposing (Element, height, px, width)
import Element.Input as Input
import Url.Builder as Builder


type alias Href =
    String


type alias Base64Image =
    String



-- Image Assets


image : String -> Href
image filename =
    Builder.absolute [ "assets", "images", filename ] []



-- Icon Assets


icon : String -> Element msg
icon identifier =
    Element.image [ height (px 16), width (px 16) ]
        { src = iconData identifier
        , description = ""
        }


iconData : String -> Base64Image
iconData identifier =
    case identifier of
        "newPost" ->
            "data:image/png;base64,"

        "search" ->
            "data:image/png;base64,"

        "tools" ->
            "data:image/png;base64,"

        "starredPosts" ->
            "data:image/png;base64,"

        "inbox" ->
            "data:image/png;base64, "

        "donate" ->
            "data:image/png;base64,"

        "discord" ->
            "data:image/png;base64,"

        "github" ->
            "data:image/png;base64,"

        "help" ->
            "data:image/png;base64,"

        "settings" ->
            "data:image/png;base64,"

        "coffee" ->
            "data:image/png;base64,"

        "ok" ->
            "data:image/png;base64,"

        "error" ->
            "data:image/png;base64,"

        "message" ->
            "data:image/png;base64,"

        _ ->
            ""



-- Misc. Assets


radio : Input.OptionState -> Element msg
radio state =
    Element.image [ height (px 24), width (px 24) ]
        { src = radioData state
        , description = ""
        }


radioData : Input.OptionState -> Base64Image
radioData state =
    case state of
        Input.Idle ->
            "data:image/png;base64,"

        Input.Focused ->
            "data:image/png;base64,"

        Input.Selected ->
            "data:image/png;base64,"
